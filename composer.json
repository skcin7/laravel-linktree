{
    "name": "skcin7/laravel-global-settings",
    "description": "A global settings package for use in a Laravel application.",
    "type": "library",
    "keywords": ["laravel", "global settings", "global", "settings"],
    "license": "MIT",
    "authors": [
    {
        "name": "Nick Morgan",
        "email": "nick@nicholas-morgan.com"
    }
    ],
    "require": {
        "php": "^5|^6|^7|^8",
        "illuminate/support": "^4.2|^5|^6|^7|^8|^9"
    },
    "require-dev": {
        "phpunit/phpunit": "^4|^5|^6|^7|^8|^9",
        "laravel/framework": ">=5.7"
    },
    "autoload": {
        "psr-4": {
            "Skcin7\\LaravelGlobalSettings\\": "src/"
        }
    },
    "extra": {
      "laravel": {
          "providers": [
              "Skcin7\\LaravelGlobalSettings\\Providers\\LaravelGlobalSettingsServiceProvider"
          ],
          "aliases": {
              "GlobalSettings": "Skcin7\\LaravelGlobalSettings\\Facades\\GlobalSettings"
          }
      }
    }


}
